<AnchorPane layoutY="20.0" prefHeight="600.0" prefWidth="182.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.dunwambank.Controllers.Client.ClientController">
   <AnchorPane prefHeight="400.0" prefWidth="168.0" styleClass="main_menu_container">
      <ImageView fitHeight="47.0" fitWidth="48.0" layoutX="32.0" layoutY="26.0" pickOnBounds="true" preserveRatio="true">
         <Image url="@../../Images/Home.png" />
      </ImageView>
      <Button fx:id="dashboard_btn" layoutX="23.0" layoutY="86.0" mnemonicParsing="false" styleClass="main_menu_container_dashboard_btn" text="Dashboard">
         <graphic>
            <FontIcon iconLiteral="fa-home" />
         </graphic>
      </Button>
      <Button fx:id="transaction_btn" layoutX="28.0" layoutY="124.0" mnemonicParsing="false" styleClass="main_menu_container_transactions_btn" text="Transactions">
         <graphic>
            <FontIcon iconLiteral="fa-handshake-o" />
         </graphic>
      </Button>
      <Button fx:id="account_btn" layoutX="26.0" layoutY="161.0" mnemonicParsing="false" styleClass="main_menu_container_accounts_btn" text="Accounts">
         <graphic>
            <FontIcon iconLiteral="fa-address-card-o" />
         </graphic>
      </Button>
      <Button fx:id="profile_btn" layoutX="27.0" layoutY="200.0" mnemonicParsing="false" styleClass="main_menu_container Profile_btn" text="Profile">
         <graphic>
            <FontIcon iconLiteral="fa-smile-o" styleClass="main_menu_container_profile_btn" />
         </graphic>
      </Button>
      <Button fx:id="logout_btn" layoutX="28.0" layoutY="237.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="75.0" styleClass="main_menu_container_logout_btn" text="Log Out">
