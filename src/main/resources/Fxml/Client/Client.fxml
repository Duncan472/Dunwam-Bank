            <FontIcon iconLiteral="fa-level-up" />
         </graphic>
      </Button>
      <AnchorPane layoutX="9.0" layoutY="277.0" prefHeight="87.0" prefWidth="170.0" styleClass="report_container">
         <Text layoutX="14.0" layoutY="27.0" strokeType="OUTSIDE" strokeWidth="0.0" styleClass="report_container Text" text="Report Suggestion/Bug?" />
         <Label layoutX="-7.0" layoutY="46.0" styleClass="report_container Label" text="Use this to report any Error" />
         <Button fx:id="report_btn" layoutX="14.0" layoutY="64.0" mnemonicParsing="false" styleClass="report_container Button" text="Report!">
            <graphic>
               <FontIcon iconLiteral="fa-plus" />
            </graphic>
         </Button>
      </AnchorPane>
      <Line endX="100.0" layoutX="79.0" layoutY="186.0" startX="-100.0" />
   </AnchorPane>
   <stylesheets>
      <URL value="@../../Styles/Client.css" />
      <URL value="@../../Styles/Client.css" />
